<templateSet group="Move">
    <template name="d"
              value="std::debug::print($TEXT$);"
              description="Debug"
              toReformat="false"
              toShortenFQNames="true">
        <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_BLOCK" value="true"/>
        </context>
    </template>

    <template name="sd"
              value='std::debug::print(&amp;std::string::utf8(b"$TEXT$"));'
              description="Debug String"
              toReformat="false"
              toShortenFQNames="true">
        <variable name="TEXT" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_BLOCK" value="true"/>
        </context>
    </template>

    <template name="t"
              value="#[test]&#10;fun $NAME$() {&#10;    $END$&#10;}"
              description="test function"
              toReformat="false">
        <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true"/>
        <context>
            <option name="MOVE_MODULE" value="true"/>
        </context>
    </template>
</templateSet>
